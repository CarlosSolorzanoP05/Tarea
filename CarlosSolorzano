funcion ejer48
	// 48. Contador de caracteres: Escribir un programa que lea una palabra y
	// presenta cuantos caracteres hay en dicha palabra
	// ent. palabra=""
	// pro. ingresar palabra
	// n=Longitud(palabra)
	// x=1
	//c=0
	// mientas x <= n
	// segun subcadena(palabra,x,x); "!"o"!" c=c+1 ....... "{}" 0 "{}"; c=c+1
	// fin se mostara el total de caracteres
	Definir palabra Como Caracter
	Escribir "*** Ejercicio 48 ***"
	Escribir "*** Contador de caracteres ***"
	Escribir "Ingrese una frase"
	leer palabra
	n=Longitud(palabra)
	x=1
	c=0
	Mientras x <= n Hacer
		segun subcadena(palabra,x,x) Hacer
			"!" o "!":
				c=c+1
			"´´" o "´´":
				c=c+1
			"#" o "#":
				c=c+1
			"$" o "$" :
				c=c+1
			"%" o "%" :
				c=c+1
			"&" o "&" :
				c=c+1
			"´" o "´" :
				c=c+1
			"()" o "()" :
				c=c+1
			"*" o "*" :
				c=c+1
			"+" o "+" :
				c=c+1
			"," o "," :
				c=c+1
			"-" o "-" :
				c=c+1
			"." o "." :
				c=c+1
			"/" o "/" :
				c=c+1
			"0" o "0" :
				c=c+1
			"1" o "1" :
				c=c+1
			"2" o "2" :
				c=c+1
			"3" o "3" :
				c=c+1
			"4" o "4" :
				c=c+1
			"5" o "5" :
				c=c+1
			"6" o "6" :
				c=c+1
			"7" o "7" :
				c=c+1
			"9" o "8" :
				c=c+1
			"9" o "9" :
				c=c+1
			":" o ":" :
				c=c+1
			";" o ";" :
				c=c+1
			"<" o "<" :
				c=c+1
			"=" o "=" :
				c=c+1
			">" o ">" :
				c=c+1
			"?" o "?" :
				c=c+1
			"@" o "@" :
				c=c+1
			"a" o "A" :
				c=c+1
			"b" o "b" :
				c=c+1
			"c" o "C" :
				c=c+1
			"d" o "d":
				c=c+1
			"e" o "e":
				c=c+1
			"f" o "f":
				c=c+1
			"g" o "g":
				c=c+1
			"h" o "H":
				c=c+1
			"i" o "I":
				c=c+1
			"j" o "J":
				c=c+1
			"k" o "K":
				c=c+1
			"l" o "L":
				c=c+1
			"m" o "M":
				c=c+1
			"n" o "N":
				c=c+1
			"o" o "O":
				c=c+1
			"p" o "P":
				c=c+1
			"q" o "Q":
				c=c+1
			"r" o "R":
				c=c+1
			"s" o "S":
				c=c+1
			"t" o "":
				c=c+1
			"u" o "":
				c=c+1
			"v" o "":
				c=c+1
			"w" o "":
				c=c+1
			"x" o "":
				c=c+1
			"y" o "":
				c=c+1
			"z" o "":
				c=c+1
			"[]" o "[]":
				c=c+1
			"_" o "_":
				c=c+1
			"{}" o "{}":
				c=c+1
		FinSegun
		x=x+1
	Fin Mientras
	Escribir "La frase ", palabra ," Tiene ", c, " Caracteres"
FinFuncion
funcion ejer49
	//49. Suma de números: Pide al usuario que ingrese números enteros positivos uno
	// por uno y utiliza un bucle while para calcular la suma de estos números. El ciclo
	// debe terminar cuando el usuario ingrese un número negativo.
	// ent.num1=0,num2=0,x=1
	// Pro. mientas x<50
	// ingresar num1,num2; suma=num1+num2
	// si num1<0 o num2<0 ; x=49  si num1<0, el numero num1 es negativo; si num2<0 elñ numero num2 es negativo
	// sino si  num1>=0 o num2>=0 ; suma ingrese otros numeros hasta que ingrese un numero negativo
	//x=x+1; FinMientras
	// fin depende la cantidad de nuemros que ingrese el usuario si son posistivos se sumara hasta que ingrese un numero negativo 
	Definir num1,num2 Como Entero
	num1=0;num2=0
	Escribir "*** Ejercicio 49 ***"
	Escribir "*** Suma de numeros ***"
	x=1
	Mientras x<50 Hacer
		Escribir "Ingrese un numero"
		leer num1
		Escribir "Ingrese un segundo numero"
		leer num2
		suma=num1+num2
		si num1<0 o num2<0 Entonces
			x=49
			si num1<0 Entonces
				Escribir "El numero " num1 " Es negativo"
			finsi
			si num2<0
				Escribir "El numero " num2 " Es negativo"
			FinSi
		sino 
			si num1>0 o num2>0 Entonces
				Escribir suma " ingrese numero hasta que los numero ingresado sean negativo"
			FinSi
	    FinSi
		x=x+1
	Fin Mientras
FinFuncion
funcion ejer50
	// 50. Cuenta regresiva: Pide al usuario que ingrese un número entero 
	// positivo y utiliza un bucle while para mostrar una cuenta regresiva
	// desde ese número hasta 1.
	// ent. num1=0
	// pro. ingresar num1 
	// x=num1
	// mientas x>1 ; si x>1 ; num1 - 1 = num1-1
	// num1=num1-1
	//x=x-1
	//fin se mostara del numero ingresado hasta el 1
	Definir num1 Como Entero
	Escribir "*** Ejercicio 50 ***"
	Escribir "*** Cuenta regresiva ***"
	Escribir "Ingrese un numero "
	leer num1 
	x=num1
	Mientras x>1 Hacer
		si x>1 Entonces
			Escribir num1 "-" 1 "= " num1-1
			num1<-num1-1
			x<-x-1
		FinSi
	Fin Mientras
FinFuncion
funcion ejer51
	// Suma de elementos: Crea un arreglo de números enteros y calcula la 
	// suma de todos sus elementos.
	// num1=0,num2=0,num3=0,num4=0, suma=0 (num1+num2+num3+num4)
	//Para i<-1 Hasta 5 Con Paso 1 Hacer
//	Escribir "Ingrese numeros"
//	leer num1,num2,num3,num4
//	suma=num1+num2+num3+num4
//	si suma<=100 Entonces
//		"Los numeros sumados son: ", suma
//		i=5
//	sino 
//		"Los numeros sumados son: ", suma
//		i=5
	Escribir "*** Ejercicio 51 ***"
	Escribir "*** Suma de elementos ***"
	Para i<-1 Hasta 5 Con Paso 1 Hacer
		Escribir "Ingrese numeros"
		leer num1,num2,num3,num4
		suma=num1+num2+num3+num4
		si suma<=100 Entonces
			Escribir "Los numeros sumados son: ", suma
			i=5
		sino 
			Escribir "Los numeros sumados son: ", suma
			i=5
		FinSi
	Fin Para
FinFuncion
funcion ejer52
	// Promedio de calificaciones: Crea un arreglo de calificaciones 
	//(números decimales) y calcula el promedio de las calificaciones.
	// ent.num1=0.0,num2=0.0,num3=0.0,promedio=0.0 (num1+num2+num3)/3
	// pro: para i=1 hasta 3 Con Paso 1 Hacer
	// "ingrese las calificaciones"
	// leer num1,num2,num3
	// promedio=(num1+num2+num3)/3
	// si promedio>0 Entonces
	// "El promedio es de: ", promedio
	// i=3
	// fin. se mostrara el promedio de las notas
	Escribir "*** Ejercicio 52 ***"
	Escribir "*** Promedio de calificaciones *** "
	para i=1 hasta 3 Con Paso 1 Hacer
		Escribir "ingrese las calificaciones"
		leer num1,num2,num3
		promedio=(num1+num2+num3)/3
		si promedio>0 Entonces
			Escribir "El promedio es de: " promedio
			i=3
		FinSi
	FinPara
FinFuncion
Funcion ejer53
	// 53.Mayor y menor valor: Encuentra el valor máximo y mínimo en un arreglo 
	// de números enteros.
	// ente mayor=0 menor=0 
	// Escribir "*** Mayor y menor valor ***"
	//Dimension A[10]
	//PARA i <- 1 hasta 10 Hacer
	//A[i]<- Azar(100)+1
	//mayor <-A[1]
	//menor <-A[1]
	//para x <-1 hasta 10 Hacer
//	si A[x]>mayor Entonces
//		mayor<-A[X]
//	si A[x]<menor Entonces
//		menor<-A[x]
	//Fin
	// El mayor valor es: ", mayor
	// "El menor valor es: ", menor
	Escribir "*** Ejercicio 53 ***"
	Escribir "*** Mayor y menor valor ***"
	Dimension A[10]
	PARA i <- 1 hasta 10 Hacer
		A[i]<- Azar(10)+1
	FinPara
	mayor <-A[1]
	menor <-A[1]
	para x <-1 hasta 10 Hacer
		si A[x]>mayor Entonces
			mayor<-A[X]
		FinSi
		si A[x]<menor Entonces
			menor<-A[x]
		FinSi
	FinPara
	Escribir "El mayor valor es: ", mayor
	Escribir "El menor valor es: ", menor
FinFuncion
funcion ejer54
	// 54 Buscar un elemento: Pide al usuario que ingrese un número y verifica si ese número está presente en un arreglo dado.
	// ent, ingresar los balores
	// ingresar otro arregro de otro ejercicio 
	// saldra el balor ingresado si esta en el otro ejercicio
	Escribir "*** Ejercicio 54 ***"
	Escribir "*** Buscar un elemto *** "
	Definir A,x,num1 Como Entero
	Dimension A[10]
	PARA i <- 1 hasta 10 Hacer
		A[i]<- Azar(10)+1
		i=i+9
	FinPara
	mayor <-A[1]
	menor <-A[1]
	para x <-1 hasta 10 Hacer
		si A[x]>mayor Entonces
			mayor<-A[X]
		FinSi
		si A[x]<menor Entonces
			menor<-A[x]
		FinSi
		x=x+9
	FinPara
	Para i<-1 Hasta 10 Con Paso 1 Hacer
		Escribir "Ingrese un numero"
		leer num1
		si num1>=mayor o num1<=menor Entonces
			Escribir "El numero ingrensado es mayor en otro ejercicio: " mayor
			i=i+9
		sino 
			Escribir "El numero ingresado es menor en otro ejercicio: " menor
			i=i+9
		FinSi
	Fin Para
FinFuncion
