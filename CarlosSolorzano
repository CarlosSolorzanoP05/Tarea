funcion ejer19
	// 19.Calculadora de BMI: Crea un programa que calcule el índice de masa corporal (BMI) a partir del peso y la altura del usuario, y luego 
	// indique si está en una categoría de peso saludable.
	// ent. altura=0.0 (metros), peso=0.0 (kilogramos) , BMI=0.0 masa/(altura*altura)
	// pro. ingreasr altura, masa
	// BMI= masa/(altura*altura)
	// si BMI>=18.25 y BMI<=24.9 ; si estas en peso sadudable 
	// sino ; no estas en paso sadudable
	// fin. depende del peso y la altura que puso el usario medir el BMI 
	Definir altura,masa,BMI Como real
	altura=0.0; masa=0.0; BMI=0.0
	Escribir "*** Ejercicio 19 ***"
	Escribir "*** Calculadora de BMI ***"
	Escribir "Ingrese su masa en kilogramos (KG)"
	leer masa
	Escribir "Ingrese su altura en metros"
	leer altura
	BMI=masa/(altura*altura)
	si BMI>=18.25 y BMI<=24.9 Entonces
		Escribir "SI esta en el BMI es sadudable", BMI
	sino 
		Escribir " NO esta en el BMI sadudable: ", BMI
	FinSi
FinFuncion
funcion ejer20
	// 20.Número positivo, negativo o cero: Pide al usuario que ingrese un número y muestra si es positivo, negativo o cero.
	// ent. num1=0, cero=0 , positivo=0 (num1>=0), negativo=0 (num1<=0)
	// pro. ingresar num1
	// si num1>=cero
	// si num1=positivo
	// es positivo 
	// sino es cero
	// si no num1<negativo ; es negativo 
	// fin. depende el numero ingresasdo de ser 0 o positivo o negativo 
	Definir num1,cero1,positivo1,negativo1 Como Entero
	num1=0;cero1=0;positivo1=0;negativo1=0
	Escribir "*** Ejercicio 20 ***"
	Escribir "*** Número Positivo *** "
	Escribir "Ingrese un número"
	leer num1
	si num1>=cero1 Entonces
		si num1=positivo1
			Escribir num1 " Es cero"
	    sino 
			Escribir num1 " Es positivo"
	    finsi
	SiNo
		si num1<negativo1
			Escribir num1 " Es negativo"
		FinSi
	FinSi
FinFuncion
funcion ejer21
	// 21.Año bisiesto: Solicita al usuario un año y determina si es un año bisiesto o no. Un año bisiesto es divisible por 4, pero no por 100,
	// a menos que también sea divisible por 400
	// ent. año=0,bis=0
	// pro ingresar año 
	// 	si año mod 100 <> 0 ; si año mod 4 =0; es Bisiesto; sino no es bisiesto
	// sino  si año mod 400 = 0; es bisesto; sino no es bisiesto 
	// fin. depende el año que ingrese el usuario que sea bisiesto o no
	Definir año como entero
	año=0 
	Escribir "*** Ejercicio 21 ***"
	Escribir "*** Año bisiesto ***"
	Escribir "Ingrese el año"
	leer año
	si año mod 100<>0 Entonces
		si año mod 4 = 0 Entonces
			Escribir año " El año es bisesto "
		sino 
			Escribir año " El año no es bisiesto "
		FinSi
	sino 
		si año mod 400=0 Entonces
			Escribir año "El año es bisiesto"
		sino 
			Escribir año " El año no es bisiesto"
		FinSi
	FinSi
FinFuncion
funcion ejer22
	// 22.Signo zodiacal: Pide al usuario que ingrese su mes y día de nacimiento, luego determina su signo zodiacal. Puedes usar una serie
	// de declaraciones if para comparar las fechas ingresadas con las fechas límite de cada signo zodiacal.
	// ent. mes=0,dia=0,x=1 
	// pro. ingresar el dia, mes
	// Si (mes==12 y (dia>=22 y dia <= 31)o(mes==1 y dia<=20)) x=1; capricornio
	// si (mes=1 y (dia>=21 y dia <= 31) o (mes=2 y dia<=19)) x=1; acuario
	// si (mes=2 y (dia>= 20 y dia<= 29) o (mes=3 y dia<=20)) x=1; picis
	// si (mes=3 y (dia>=21 y dia<= 31) o (mes=4 y dia<=20))x=1; aries
	// si (mes=4 y (dia>=21 y dia<=30) o (mes=5 y dia <= 20))x=1; tauro 
	// si (mes=5 y (dia>=1 y dia<=31) o (mes=6 y dia<=21))x=1; gemesis
	// si (mes=6 y(dia>=22 y dia<=30) o (mes=7 y dia <=22))x=1; cancer
	// si (mes=7 y (dia>=23 y dia<=31) o (mes=8 y dia<=22))x=1; leo 
	// si (mes=8 y (dia>23 y dia<=31) o (mes=9 y dia<=22))x=1; virgo
	//si (mes=9 y (dia>=23 y dia<=31) o (dia=10 y dia<=22))x=1; libra 
	// si (mes=10 y(dia>=23 y dia<= 31) o (mes=11 y dia<=22))x=1; escorpion
	// si (mes=11 y(dia>=23 y dia <= 30) o (mes=12 y dia>=21))x=1; sagitario
	// si x=1 tu signo zodiacal es: signo
	// sino el signo no existe 
	// final. mostar el signo zodiacal que tiene el usuario
	Definir dia,mes,c como entero
	Definir signo Como Caracter
	dia=0; mes=0;c=0;signo=""
	Escribir "*** Ejercicio 22 ***"
	Escribir "*** Singno zodiacal ***"
	Escribir 'ingrese su mes de nacimiento'
	Leer mes
	Escribir 'ingrese su dia de nacimiento'
	Leer dia
	Si (mes==12 y (dia>=22 y dia <= 31)o(mes==1 y dia<=20)) Entonces
		signo= "capricornio"
		c=1
	FinSi
	si (mes=1 y (dia>=21 y dia <= 31) o (mes=2 y dia<=19)) Entonces
		signo= "Acuario"
		c=1
	FinSi
	si (mes=2 y (dia>= 20 y dia<= 29) o (mes=3 y dia<=20)) Entonces
		signo ="Picis"
		c=1
	FinSi
	si (mes=3 y (dia>=21 y dia<= 31) o (mes=4 y dia<=20)) Entonces
		signo="Aries"
		c=1
	FinSi
	si (mes=4 y (dia>=21 y dia<=30) o (mes=5 y dia <= 20))Entonces
		signo="tauro"
		c=1
	FinSi
	si (mes=5 y (dia>=1 y dia<=31) o (mes=6 y dia<=21)) Entonces
		signo="Gemenis"
		c=1
	FinSi
	si (mes=6 y(dia>=22 y dia<=30) o (mes=7 y dia <=22)) Entonces
		signo="cancer"
		c=1
	FinSi
	si (mes=7 y (dia>=23 y dia<=31) o (mes=8 y dia<=22)) Entonces
		signo="leo"
		c=1
	FinSi
	si (mes=8 y (dia>23 y dia<=31) o (mes=9 y dia<=22)) Entonces
		signo="virgo"
		c=1
	FinSi
	si (mes=9 y (dia>=23 y dia<=31) o (dia=10 y dia<=22)) Entonces
		signo="libra"
		c=1
	FinSi
	si (mes=10 y(dia>=23 y dia<= 31) o (mes=11 y dia<=22)) Entonces
		signo="escorpion"
		c=1
	FinSi
	si (mes=11 y(dia>=23 y dia <= 30) o (mes=12 y dia>=21)) Entonces
		signo="sagitario"
		c=1
	FinSi
	si c=1 Entonces
		Escribir "tu signo del zodiaco es: ", signo
	SiNo
		Escribir "el signo del zodiaco no existe"
	FinSi
FinFuncion
funcion ejer23
	// 23.Día del mes con respecto a la segunda quincena: Solicita al usuario que ingrese un número de día del mes (por ejemplo, del 1 al 31) y 
	// verifica si ese día pertenece a la primera quincena (días 1-15) o a la segunda quincena (días 16-31).
	// ent. dia=0, priqin=0(Dia=1-15), segquin=0(dia=16-31)
	// pro ingrear dia
	//si dia>=1 y dia<15; primera quinsena
	// sino si dia>=16 y dia<=31 ; segunda quinsena
	// fin. depende el dia que puso el usuario si es primea quisena o segunda quinsena
	Definir dia Como Entero
	dia=0
	Escribir "*** Ejercicio 23 ***"
	Escribir "*** Día del mes respecto a la segunda quinsena ***"
	Escribir "Ingrese un dia del mes"
	leer dia
	Si (dia>=1 Y dia<=15) Entonces
		Escribir 'Primera quinsena'
	SiNo
		Si (dia>=16 Y dia<=31) Entonces
			Escribir 'segunda quinsena'
		FinSi
	FinSi
FinFuncion
